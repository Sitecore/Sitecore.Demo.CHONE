{
  "name": "play-media",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tenant:add": "ch-one-cli tenant add",
    "tenant:list": "ch-one-cli tenant list",
    "ser:pull-content-types": "cd items && ch-one-cli ser pull content-type",
    "ser:push-content-types": "cd items && ch-one-cli ser push content-type",
    "ser:pull-content-items": "cd items && ch-one-cli ser pull content-item -c Sport && ch-one-cli ser pull content-item -c Athlete && ch-one-cli ser pull content-item -c Event",
    "ser:push-content-items": "cd items && ch-one-cli ser push content-item -c Sport && ch-one-cli ser push content-item -c Athlete && ch-one-cli ser push content-item -c Event",
    "publish-item": "cd items && ch-one-cli delivery content publish",
    "publish-all-items": "ts-node --project tsconfig.scripts.json scripts/publishContentItems.ts",
    "remove-media-fields": "ts-node --project tsconfig.scripts.json scripts/removeMediaFromContentItems.ts"
  },
  "dependencies": {
    "@types/node": "18.11.10",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "clsx": "^1.2.1",
    "color-hash": "^2.0.2",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "next": "13.0.6",
    "postcss-import": "^15.0.1",
    "postcss-nested": "^6.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-parallax": "^3.5.1",
    "slugify": "^1.6.5",
    "swiper": "^8.4.5",
    "typescript": "4.9.3"
  },
  "devDependencies": {
    "@types/color-hash": "^1.0.2",
    "@types/js-yaml": "^4.0.5",
    "autoprefixer": "^10.4.13",
    "js-yaml": "^4.1.0",
    "postcss": "^8.4.19",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1"
  }
}
